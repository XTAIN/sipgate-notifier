{
    "name": "xtain/sipgate-notifier",
    "type": "symfony-bridge",
    "description": "Symfony Sipgate Notifier Bridge",
    "keywords": ["sms", "sipgate", "notifier"],
    "homepage": "https://xtain.net",
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "symfony/http-client": "^4.3|^5.0",
        "symfony/notifier": "^5.1"
    },
    "autoload": {
        "psr-4": { "XTAIN\\Notifier\\Bridge\\Sipgate\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
